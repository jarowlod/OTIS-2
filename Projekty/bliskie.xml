// XML ze schowka
procedure TForm1.Button5Click(Sender: TObject);
var FDoc: TXMLDocument;
    s: string;
    Strm: TStringStream;
    i: integer;
    NodeCOB: TDOMNodeList;
begin
  s:= Clipboard.AsText;
  try
    Strm:= TStringStream.Create(s);
    ReadXMLFile(FDoc, Strm);

    NodeCOB:= FDoc.GetElementsByTagName('COB');
    if Assigned(NodeCOB) then
    for i:=0 to NodeCOB.Count-1 do
      Memo1.Append( NodeCOB[i].Attributes.GetNamedItem('IME').NodeValue);

  finally
    FDoc.Free;
    Strm.Free;
  end;
end;   